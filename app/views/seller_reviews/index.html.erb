<p id="notice"><%= notice %></p>

<h1>Listing Seller Reviews</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seller_reviews.each do |seller_review| %>
      <tr>
        <td><%= link_to 'Show', seller_review %></td>
        <td><%= link_to 'Edit', edit_seller_review_path(seller_review) %></td>
        <td><%= link_to 'Destroy', seller_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller review', new_seller_review_path %>
