<%if @products.count !=0 %>
$("#page_entry_id").show();
$("#page_entry_id").html("<%= escape_javascript(page_entries_info @products,:entry_name => '')%>");
$("#ajaxproducts").html("<%= escape_javascript(render partial: "products/product", collection: @products, layout: "product_wrapper_layout",:locals => {:size => "col-md-4",obj:"browse"}
) %>");
$("#paginateid").html("<%= escape_javascript(paginate @products,theme: 'site_pagination',:params=>{:sortid=>@sortvalue, :textmin=>@textminvalue, :textmax=>@textmaxvalue, :brand_ids=>@selected_brands },:remote => true
) %>");
<%else%>
$("#page_entry_id").hide();
$("#ajaxproducts").html("<%= escape_javascript(render partial: "noproductfound")%>");
$("#paginateid").html("<%= escape_javascript(paginate @products,theme: 'site_pagination',:params=>{:sortid=>@sortvalue, :textmin=>@textminvalue, :textmax=>@textmaxvalue, :brand_ids=>@selected_brands },:remote => true
) %>");
<%end%>